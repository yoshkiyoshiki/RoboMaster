{
    "name": "FreeRTOS-Teensy4",
    "version": "10.0.5",
    "keywords": "rtos, freertos, teensy4",
    "description": "Teensy 4.0 FreeRTOS Library for Teensyduino",
    "repository":
    {
      "type": "git",
      "url": "https://github.com/discord-intech/FreeRTOS-Teensy4.git"
    },
    "authors":
    [
      {
        "name": "Julian Desvignes",
        "email": "julian.desvignes@rakuten.com",
        "maintainer": true
      }
    ],
    "export": {
        "include":
        [
            "src/*",
            "examples/*"
        ]
    },
    "frameworks": "arduino",
    "platforms": "teensy",
    "build":
    {
      "flags": "-mno-unaligned-access",
      "libArchive": false,
      "includeDir": "../../include/"
    }
}